{
  "name": "xtendui",
  "version": "0.16.9",
  "description": "Powerful frontend library with Tailwind components and vanilla Javascript.",
  "homepage": "http://xtendui.com/",
  "license": "MIT",
  "author": "Riccardo Caroli <beaver82@gmail.com>",
  "repository": "https://github.com/minimit/xtendui.git",
  "keywords": [
    "frontend",
    "library",
    "components",
    "vanilla",
    "tailwind"
  ],
  "main": "dist/xtend.min.js",
  "module": "src/xt.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/*.min.*",
    "src/*.*",
    "!src/html.js",
    "src/addons/*.*",
    "src/core/*.*"
  ],
  "scripts": {
    "webpack": "cross-env NODE_ENV=development webpack --progress --colors",
    "build": "gatsby clean && gatsby build",
    "dev": "gatsby clean && gatsby develop -p 8888",
    "serve": "cross-env NODE_ENV=production yarn build && gatsby serve -p 8888",
    "deploy": "cross-env NODE_ENV=production yarn build && gh-pages -d public",
    "release": "npx semantic-release --dry-run --no-ci",
    "lint": "eslint . --ext .js",
    "postversion": "yarn webpack"
  },
  "dependencies": {
    "@googlemaps/markerclustererplus": "^1.0.3",
    "core-js": "^3.6.5",
    "gsap": "^3.3.4",
    "json5": "^2.1.3",
    "tailwindcss": "^1.8.12"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@hot-loader/react-dom": "^16.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "babel-loader": "^8.0.6",
    "clipboard": "^2.0.4",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "esm": "^3.2.25",
    "gatsby": "^2.24.72",
    "gatsby-plugin-catch-links": "^2.3.10",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-page-creator": "^2.3.15",
    "gatsby-plugin-postcss": "^3.0.1",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-remark-classes": "^1.0.0",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-custom-blocks": "^2.3.9",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-remark": "^2.8.23",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "indent-string": "^4.0.0",
    "lodash.castarray": "^4.4.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.3",
    "postcss-nested": "^5.0.1",
    "prettier-eslint": "^11.0.0",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "rehype-react": "^6.0.0",
    "remark-grid-tables": "^2.1.1",
    "semantic-release": "^17.1.1",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.12",
    "write": "^2.0.0"
  }
}
